//(1)
#include <stdio.h>
int main() {
	int pow, coe, i, j,k;
	int a[101] = { 0 }, b[101] = { 0 }, c[101] = { 0 };
	do
	{
		scanf("%d %d", &pow, &coe);
		a[pow] = coe;
	} while (pow != 0);
	do
	{
		scanf("%d %d", &pow, &coe);
		b[pow] = coe;
		c[pow] = b[pow] + a[pow];
		a[pow] = 0;
	} while (pow != 0);
	for (i = 100; i >= 0; i--)
	{
		if (a[i] != 0)
			c[i] = a[i];
	}
	for (k = 100; k > 1; k--)
	{
		if (c[k] != 0) 
		{
			if (c[k] == 1 )
				printf("x%d", k);
			else if(c[k]==-1)
				printf("-x%d",k);
			else
				printf("%dx%d", c[k], k);
			break;
		}
	}
	for (j=k-1; j > 1; j--)
	{
		if (c[j] > 0 && c[j] != 1)
			printf("+%dx%d", c[j], j);
		else if (c[j] < 0 && c[j]!=1)
			printf("%dx%d", c[j], j);
		else if (c[j] == 1)
			printf("+x%d", j);
		else if (c[j]==-1)
			printf("-x%d", j);
	}
	if(k>1)
	{
		if (c[1] != 0)
		{
			if(c[1]==1)
		    printf("+x");
		else if(c[1]>0)
			printf("+%dx",c[1]);
		else if(c[1]==-1)
			printf("-x");
		else
			printf("%dx",c[1]);
		}				
	}
	else
	{
		if (c[1] != 0)
		{
			if(c[1]==1)
		    printf("x");
		else if(c[1]>0)
			printf("%dx",c[1]);
		else if(c[1]==-1)
			printf("-x");
		else
			printf("%dx",c[1]);
		}
	}
	if (c[0] > 0&&j!=0)
		printf("+%d", c[0]);
	else if (j==0)
		printf("%d", c[0]);
	return 0;
}
