//(1)
#include<stdio.h>
int main(){
	int i=0,j=0,a;
	do
	{
		scanf("%d",&a);
		if(a==-1) break;
		if(a%2==0)
			i++;
		else
			j++;
	}while(a>0&&a<100000);
	printf("%d %d\n",j,i);
	return 0;
}


//(2)
#include <stdio.h>
#include<math.h>
int main() {
    int a,i,j=-1;
    int sum=0;
    scanf("%d",&a);
    while(a>=0)
    {
    if((a%10)%2==0) {j++;i=0;}
    else {j++;i=pow(2,j);};
    sum+=i;
    a/=10;
    if(a<=0) break;
    if((a%10)%2==0) {j++;i=pow(2,j);}
    else {j++;i=0;}
    sum+=i;
    a/=10;
    }

    printf("%d\n",sum);
    return 0;
}
