//(1)
#include<stdio.h>
int main(){
	int t,a;
	scanf("%d",&t);
	if(t>=800&&t<=2359)
		a=t-800;
	else if(t<800&&t>=0)
		a=t+1600;
	printf("%d",a);
	return 0;
}



//(2)
#include<stdio.h>
int main(){
    int n,i;
	scanf("%d",&n);
	printf("1");
	for(i=3;i<=n;i=i+2){
		printf(" ");
		printf("%d",i);
		
	}
	return 0;
}
